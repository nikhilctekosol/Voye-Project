	<section class="inner-wrapper-nospace">

		<div class="contact-wrapper">
			<div class="half-layout">
				<div class="left">
					<div class="form-wrap booking">
					<form novalidate="" asp-controller="About" asp-action="Contact" method="post">
						<h4 class="subtitles pb-2">Get in touch</h4>
						<div class="fields name">
							<label for="full_name"><span>Guest name</span></label>
							<input type="text" name="full_name" placeholder="Mr. Nicolas" class="form-field" />
						</div>
						<div class="fields name">
						<label for="email"><span>E-mail</span></label>
						<input type="text" name="email" placeholder="name@email.com" class="form-field" />
						</div>
						<div class="fields name">
						<label for="mobile"><span>Phone</span></label>
						<input type="text" name="mobile" placeholder="+971 000 000000" class="form-field" />
						</div>
						<div class="fields name">
						<label for="details"><span>Message</span></label>
						<textarea type="text" name="details" placeholder="Message" class="form-field"></textarea>
						</div>
						<button class="btn btn-primary c-brand" type="submit">Send a message</button>
						@{
							var successMessage = TempData["ContactSuccess"] as string;
						}
						@if (!string.IsNullOrEmpty(successMessage))
						{
							<div class="alert alert-success" style="margin-top:5px;">
								@successMessage
							</div>
						}
					</form>
				</div>
			</div>
			<div class="right">
				<div class="address">
					<h4 class="subtitles pb-2">Location </h4>
					<p>VOYE VACATION HOME RENTAL LLC</p>
					<p>Office SF 37</p>
					<p>DUBAI SHOPPING CENTER SF-37</p>
					<p>Behind Deira City Centre</p>
					<p>Dubai, UAE</p>
				</div>
			</div>
			</div>
		</div>

	</section>




